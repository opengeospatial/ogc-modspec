ifier>/req/core/extensions-conformant-to-the-modspec</identifier></requirement><requirement id="_a63b7718-7131-b59a-03d0-c890da97232b" model="ogc" type="general"> <identifier>/req/core/restriction-of-extensions</identifier></requirement><requirement id="_45bb4d78-4d68-21ae-3df4-ce5d043a6e82" model="ogc" type="general"> <identifier>/req/core/optional-requirements</identifier></requirement><requirement id="_4cef30f9-3f0f-3c2c-18e5-a94c236649d0" model="ogc" type="general"> <identifier>/req/core/req-class-overlap-by-reference</identifier></requirement><requirement id="_92b62f6c-01e6-c1e8-0c91-481e988c4baa" model="ogc" type="general"> <identifier>/req/core/reqs-are-in-class</identifier></requirement>

